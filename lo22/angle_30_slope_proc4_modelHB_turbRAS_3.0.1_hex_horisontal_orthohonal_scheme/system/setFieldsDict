FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
);

regions
(
	rotatedBoxToCell
    {
		origin	(50 150 0);
		i		(350 0 0);
		j		(0 200 0);
		k		(0 0 5);
        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }
);


